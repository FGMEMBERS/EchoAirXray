  </model>
    <model>
    <path>Aircraft/EchoAirTraveller/Models/MFD/mfd.xml</path>
    <offsets>
      <y-m>0.1</y-m>
      <x-m>-0.00</x-m>
      <z-m>-0.02</z-m>
<pitch-deg>90</pitch-deg>
<roll-deg>90</roll-deg>
    </offsets>
  </model>

