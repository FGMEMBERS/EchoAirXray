<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertyList>
<path>panel2.ac</path>
  <animation>
    <type>material</type>
    <object-name>panel</object-name>
    <condition>
<and>
      <property>/controls/electric/battery-switch</property>
<property>/controls/lighting/instrument-lights</property>
</and>
    </condition>
    <emission>
      <red>   0.1 </red>
      <green> 0.1 </green>
      <blue>  0.1 </blue>
    </emission>
  </animation>

<!--Uncatagorized-->
