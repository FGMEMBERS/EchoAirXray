      <z-m>-0.165</z-m>
      <x-m>-0.004</x-m>
    </offsets>
  </model>


<!--Flaps lever-->
   <model>
    <path>Instruments/flaps/flaps.ac</path>
    <offsets>
      <y-m>-0.0</y-m>
      <z-m>-0.73</z-m>
      <x-m>0.66</x-m>
<pitch-deg>30</pitch-deg>
    </offsets>
  </model>
<animation>
<type>rotate</type>
<object-name>flapslever</object-name>
<property>/controls/flight/flaps</property>
<factor>60</factor>
<axis><y>1</y></axis>
<center>
<x-m>0.0</x-m>
<y-m>0.0</y-m>
<z-m>0.0</z-m>
</center>
</animation>
<!--Throttle lever-->
   <model>
    <path>Instruments/throttle/throttle.ac</path>
    <offsets>
      <y-m>-0.0</y-m>
      <z-m>-0.65</z-m>
      <x-m>0.4</x-m>
<pitch-deg>30</pitch-deg>
    </offsets>
  </model>
<animation>
<type>rotate</type>
<object-name>throttlelever</object-name>
<object-name>throttlecylinder</object-name>
<property>/controls/engines/engine/throttle</property>
<factor>-60</factor>
<axis><y>1</y></axis>
<center>
<x-m>0.0</x-m>
<y-m>0.0</y-m>
<z-m>0.0</z-m>
</center>
</animation>

<!--Checklists-->
<animation>
<type>pick</type>
    <object-name>checklists</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/sim/model/checklists</property>
      </binding>
        <binding>
          <command>dialog-show</command>
          <dialog-name>checklist</dialog-name>
        </binding>
    </action>
</animation>
<animation>
<type>select</type>
    <object-name>checklists</object-name>  
      <condition>
<not>
        <property>/sim/model/checklists</property>
</not>
      </condition>
</animation>
<animation>
<type>pick</type>
    <object-name>checklistsout</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/sim/model/checklists</property>
      </binding>
    </action>
</animation>
<animation>
<type>select</type>
    <object-name>checklistsout</object-name>  
      <condition>
        <property>/sim/model/checklists</property>
      </condition>
</animation>
 <effect>
    <inherits-from>Aircraft/EchoAirTraveller/Models/Effects/interior/c172p-interior</inherits-from>
          <object-name>panel</object-name>
          <object-name>base</object-name>
          <object-name>switch</object-name>
          <object-name>throttlebase</object-name>
          <object-name>throttlelever</object-name>
          <object-name>throttlecylinder</object-name>
          <object-name>flapsbase</object-name>
          <object-name>flapslever</object-name>
  </effect>
</PropertyList>
