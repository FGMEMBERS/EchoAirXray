<!--Flaps lever-->
   <model>
    <path>Instruments/flaps/flaps.ac</path>
    <offsets>
      <y-m>-0.0</y-m>
      <z-m>-0.5</z-m>
      <x-m>0.8</x-m>
<pitch-deg>20</pitch-deg>
    </offsets>
  </model>
<animation>
<type>rotate</type>
<object-name>flapslever</object-name>
<property>/surface-positions/flap-pos-norm</property>
<factor>60</factor>
<axis><y>1</y></axis>
<center>
<x-m>0.0</x-m>
<y-m>0.0</y-m>
<z-m>0.0</z-m>
</center>
</animation>
<!--Throttle lever-->
   <model>
    <path>Instruments/throttle/throttle.ac</path>
    <offsets>
      <y-m>-0.0</y-m>
      <z-m>-0.4</z-m>
      <x-m>0.5</x-m>
<pitch-deg>20</pitch-deg>
    </offsets>
  </model>
<animation>
<type>rotate</type>
<object-name>throttlelever</object-name>
<object-name>throttlecylinder</object-name>
<property>/controls/engines/engine/throttle</property>
<factor>-60</factor>
<axis><y>1</y></axis>
<center>
<x-m>0.0</x-m>
<y-m>0.0</y-m>
<z-m>0.0</z-m>
</center>
</animation>
<!--Checklists-->
<animation>
<type>pick</type>
    <object-name>checklists</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>/sim/model/checklists</property>
      </binding>
        <binding>
          <command>dialog-show</command>
          <dialog-name>checklist</dialog-name>
        </binding>
    </action>
</animation>

